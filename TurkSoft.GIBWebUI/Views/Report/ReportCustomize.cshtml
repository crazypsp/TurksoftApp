@{
    ViewData["Title"] = "Rapor Özelleştir";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!-- Sayfaya özgü ufak stiller (section yerine inline bırakıldı) -->
<style>
    .box .box-title {
        font-weight: 600;
    }

    .dt-head-center thead th {
        text-align: center;
    }

    .panel-sky {
        background: transparent;
        border: none;
    }

        .panel-sky .panel-body {
            padding: 0;
        }
</style>

<script>
    // View kapsamındaki veri (gerekirse Controller'dan Model olarak da verilebilir)
    // Not: Orijinal sayfadaki örnek dizi korundu
    window.raporTurList = [{"id":1,"name":"e-Fatura Gelen"},{"id":2,"name":"e-Fatura Giden"},{"id":3,"name":"e-Arşiv Fatura"},{"id":4,"name":"Tüm Giden Faturalar"},{"id":5,"name":"e-Serbest Meslek Makbuz"},{"id":6,"name":"e-Müstahsil Makbuz"},{"id":8,"name":"e-İrsaliye Gelen"},{"id":9,"name":"e-İrsaliye Giden"},{"id":11,"name":"e-Adisyon"}];
