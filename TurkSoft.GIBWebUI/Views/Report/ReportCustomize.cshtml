    <script>
        function collectSelectedFromStaticTree(){
          var ids = [];
          $('#modal-TasarimEkle #mytree li.jstree-node > a.jstree-anchor').each(function () {
            var $a = $(this);
            var selected = $a.attr('aria-selected') === 'true' || $a.hasClass('jstree-clicked');
            if (selected) {
              var id = ($a.closest('li').attr('id')) || ($a.attr('id') || '').replace('_anchor','');
              if (id) ids.push(id);
            }
          });
          return ids;
        }

        // Kaydet
        function tasarimEkleBtn(){
          var $m = $('#modal-TasarimEkle'); if (!$m.length) return;
          var payload = {
            templateName: $m.find('#templateName').val(),
            reportType: $m.find('#ReportType').val(),
            // Seçimleri statik HTML’den alıyoruz
            fields: collectSelectedFromStaticTree()
          };

          $.ajax({
            url:'/ReportCustomize/Save',
            method:'POST',
            contentType:'application/json; charset=utf-8',
            data: JSON.stringify(payload)
          })
          .done(function(){
            if (window.toastr) toastr.success('Kaydedildi');
            $m.modal('hide');
            window.myReportDT && window.myReportDT.ajax.reload(null,false);
          })
          .fail(function(){ if(window.toastr) toastr.error('Kaydedilemedi'); });
        }
    </script>

