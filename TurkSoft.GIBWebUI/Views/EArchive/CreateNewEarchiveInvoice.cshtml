              // 1) header'daki THEAD'i asıl tabloya taşı
              var thead = headTable.querySelector('thead');
              if (thead) {
                bodyTable.insertBefore(thead, bodyTable.firstChild);
              }

              // 2) DataTables'ın eklediği inline width/min/max'ları temizle
              bodyTable.querySelectorAll('th,td').forEach(function(el){
                el.style.width = '';
                el.style.minWidth = '';
                el.style.maxWidth = '';
              });

              // 3) Sarmalayıcıları kaldırıp tek tablo bırak
              if (scrollHead) scrollHead.remove();
              if (scrollBody) {
                var parent = scrollBody.parentNode;
                parent.insertBefore(bodyTable, scrollBody);
                scrollBody.remove();
              }

              // 4) Sabit ve esnek kolon genişlikleri — toplamı yatayı taşırmaz
              //    2. kolon (Mal/Hizmet Adı) esnek bırakıldı.
              var widths = ['60px','auto','100px','100px','110px','80px','110px','110px','60px','110px','100px','120px'];

              // Eski colgroup varsa sil
              var oldCg = bodyTable.querySelector('colgroup');
              if (oldCg) oldCg.remove();

              // Yeni colgroup ekle
              var cg = document.createElement('colgroup');
              widths.forEach(function(w){
                var c = document.createElement('col');
                c.style.width = w;
                cg.appendChild(c);
              });
              bodyTable.prepend(cg);

              // 5) Tablo düzeni
              bodyTable.style.tableLayout = 'fixed';
              bodyTable.style.width = '100%';
            })();
    </script>

 





